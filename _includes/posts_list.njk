<ol reversed>
	{%- for post in posts_list | reverse %}
		<li>
			<a href="{{ post.url }}">
				{% if post.data.title %}
					{{ post.data.title }} rawr
				{% else %}
					Untitled
				{% endif %}
			</a>
			<time datetime="{{ post.date | htmlDateString }}">
				{{ post.date | readableDate("LLLL yyyy") }}
			</time>
		</li>
	{%- endfor %}
</ol>
